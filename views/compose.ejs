<%- include("partials/header") -%>
    <h1>Compose</h1>
    <form method="post" action="/compose">
        <div class="form-group">
            <label><b>Title</b></label>
            <input class="form-control" type="text" name="postTitle" placeholder="Compose your title!">
            <label><b>Post</b></label>
            <textarea class="form-control" name="postBody" rows="5" cols="30" placeholder="Compose your post!">

            </textarea>
        </div>
        <button class="btn btn-primary" type="submit">Publish</button>
    </form>