<%- include("partials/header") -%>
<h1>Home</h1>
<p><%= startingContent %></p>
    <%posts.forEach(function(post){ %>
        <h2><%=post.title%></h2>
        <p><%=post.content.substring(0,100)+" ..."%><a href="/posts/<%= post.title%>">Read More</a></p>
    <%})%>
    <form action="/compose">
        <button class="btn btn-lg btn-primary btn-outline-dark btn-bd-primary" type="submit">Compose New Post!</button>
    </form>
<%- include("partials/footer") -%>
